{
	"semi": true,
	"singleQuote": false,
	"singleAttributePerLine": false,
	"htmlWhitespaceSensitivity": "css",
	"printWidth": 80,
	"plugins": [
		"prettier-plugin-tailwindcss"
	],
	"tailwindFunctions": [
		"clsx",
		"cn"
	],
	"tabWidth": 2,
	"overrides": [
		{
			"files": "**/*.yml",
			"options": {
				"tabWidth": 2
			}
		}
	]
}